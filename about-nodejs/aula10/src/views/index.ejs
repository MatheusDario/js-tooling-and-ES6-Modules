<%- include('includes/head'); %>

  <section class="container">
    <h1>
      <%= titulo %>
    </h1>

    <p>
      <% numeros.forEach(num => { %>
        <%= num %><br>
      <% }); %>
    </p>

    <form action="/" method="post">
      <input type="hidden" name="_csrf" value="<%= csrfToken %>">
      <label>Cliente</label>
      <input type="text" id="cliente" name="cliente">
      <button>Enviar</button>
    </form>
  </section>

<%- include('includes/footer'); %>