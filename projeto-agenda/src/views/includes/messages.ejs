<% if(errors.length > 0) {  %>
<div class="row">
    <div class="col my-3">
      <div class="alert alert-danger text-center">
          <% errors.forEach(errors => { %>
            <%= errors %><br>
          <% }); %>
      </div>
    </div>
  </div>
<% } %>

<% if (success.length > 0) { %>
  <div class="row">
    <div class="col my-3">
      <div class="alert alert-success">
        <% success.forEach(success => { %>
        <%= success %><br>
        <% }); %>
      </div>
    </div>
  </div>
  <% } %>

<% if(successLogin.length > 0) {  %>
    <div class="row">
        <div class="col my-3">
          <div class="alert alert-success text-center">
              <p>Login efetuado com Sucesso</p>
          </div>
        </div>
      </div>
<% } %>

<% if(successContato.length > 0) {  %>
  <div class="row">
      <div class="col my-3">
        <div class="alert alert-success text-center">
            <p>Contato cadastrado com Sucesso</p>
        </div>
      </div>
    </div>
<% } %>

<% if(contatoEditado.length > 0) {  %>
  <div class="row">
      <div class="col my-3">
        <div class="alert alert-success text-center">
            <p>Contato editado com Sucesso</p>
        </div>
      </div>
    </div>
<% } %>

<% if(deleteContato.length > 0) {  %>
  <div class="row">
      <div class="col my-3">
        <div class="alert alert-success text-center">
            <p>Seu contato foi Deletado com Sucesso</p>
        </div>
      </div>
    </div>
<% } %>